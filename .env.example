# Supabase
NEXT_PUBLIC_SUPABASE_URL="https://your-project-id.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Kakao API (Optional - for future implementation)
KAKAO_CLIENT_ID="your-kakao-client-id"
KAKAO_CLIENT_SECRET="your-kakao-client-secret"
KAKAO_REDIRECT_URI="http://localhost:3000/api/auth/callback/kakao"

# ML Service (Python FastAPI)
# Python ML 서비스 URL (docker-compose로 실행 시 http://localhost:8000)
ML_SERVICE_URL="http://localhost:8000"
# ML 서비스 요청 타임아웃 (밀리초)
ML_SERVICE_TIMEOUT="10000"
# ML 서비스 사용 여부 (false로 설정하면 TypeScript 알고리즘만 사용)
ML_SERVICE_ENABLED="true"

# App Configuration
NODE_ENV="development"

# Conductor Notes Encryption
# 지휘자 메모 암호화 키 (64자리 16진수 문자열)
# 다음 명령으로 생성: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
CONDUCTOR_NOTES_ENCRYPTION_KEY="your-64-character-hex-encryption-key"
